{
  "files.exclude": {
    "**/node_modules": false
  },
  "search.exclude": {
    "**/node_modules": false
  },
  "files.watcherExclude": {
    "**/node_modules/**": true
  },
  "npm.packageManager": "npm",
  "npm.enableScriptExplorer": true,
  "security.workspace.trust.enabled": true,
  "security.workspace.trust.startupPrompt": "always",
  "terminal.integrated.inheritEnv": false
}
